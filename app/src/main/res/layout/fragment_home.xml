        android:layout_marginStart="96dp"
        android:layout_marginTop="12dp"
        android:text="Meat"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/main_fragment_cookies_text_view"
        app:layout_constraintTop_toBottomOf="@+id/main_fragment_deserts_image_view" />

    <TextView
        android:id="@+id/main_fragment_healthy_food_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="432dp"
        android:text="No crab"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/main_fragment_salads_text_view"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/main_fragment_mains_text_view"
        android:layout_width="88dp"
        android:layout_height="27dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="52dp"
        android:text="Sushi"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>